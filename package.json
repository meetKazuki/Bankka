  "scripts": {
    "clean": "rm -rf .nyc_output build coverage && mkdir build",
    "db:migrate": "npm run sequelize-cli:ts db:migrate",
    "db:rollback": "npm run db:undo && npm run db:migrate",
    "db:seed": "npm run sequelize-cli:ts db:seed:all",
    "db:undo": "npm run sequelize-cli:ts db:migrate:undo:all",
    "dev": "DEBUG=dev tsc-watch --onSuccess \"node ./build/src/server.js\"",
    "doc:generate": "npm run build && swagger-jsdoc -d dist/swagger.ts  -o ./docs/swagger.json",
    "sequelize-cli:es6": "babel-node ./node_modules/.bin/sequelize $*",
    "sequelize-cli:ts": "./sequelize.sh $*",
    "test": "DEBUG=test mocha"
  },
